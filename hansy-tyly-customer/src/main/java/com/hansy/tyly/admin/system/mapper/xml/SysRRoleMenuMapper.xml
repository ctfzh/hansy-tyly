<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hansy.tyly.admin.system.mapper.SysRRoleMenuMapper">
	<update id="updateMenuRoleStatus">
		UPDATE sys_r_role_menu set STATUS=#{status},UPDATE_TIME=now() where
		ROLE_ID=#{roleId}
	</update>
</mapper>